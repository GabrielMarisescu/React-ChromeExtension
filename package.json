{
  "name": "react-chrome",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "ignore": [
        "dist",
        "node_modules",
        ".vscode"
      ],
      "extensions": [
        "tsx",
        "ts",
        "js",
        "jsx",
        "json"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "INLINE_RUNTIME_CHUNK=false tsc && vite build",
    "preview": "vite preview",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.2",
    "npm-watch": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.203",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "jest": "^29.3.1",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
